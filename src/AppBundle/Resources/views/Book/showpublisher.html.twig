{% extends 'base.html.twig' %}
{% block stylesheets %}
  <style>
    table, th, td {
      border: 1px solid black;
      border-collapse: collapse;
      padding: 5px;
      text-align: left;
      margin-top: 20px;
    }
  </style>
{% endblock %} 
{% block body %}
  <div class="container">
    <table class="table table-striped">
      <thead class="thead-dark">
        <tr>    
         <th>Title</th>
         <th>City</th>                  
        </tr>
      </thead>
      <tr>    
        <td>{{ publisher.title }}</td>
        <td>{{ publisher.city }}</td>  
      </tr>      
    </table>
  </div>      
{% endblock %}